/* pages/patient/homepage/home/home.wxss */
.search-back{
  background:#ffffff;
  width:100%;
  height:100rpx;
  padding:15rpx 20rpx 15rpx 20rpx;
  position: fixed;
}
.search-box{
  background:#eeeeee;
  width:100%;
  height:70rpx;
  display:flex;
  align-items: center; 
  border-radius: 35rpx;
}
.search-icon{
  margin:0 10rpx 0 30rpx;
}

.content{
  padding:0 30rpx 0 30rpx;
  margin-top:20rpx;
  margin-bottom:100rpx;
}
.text{
  line-height:80rpx;
  font-size:40rpx;
  font-weight: 600;
}
.box{
  background:#ffffff;
  border-radius:20rpx;
  margin-bottom: 25rpx;
  padding:20rpx;
}
.title{
  line-height:80rpx;
  font-size:30rpx;
  font-weight: 600;
}
.nav-list{
  margin:0 -20px 0 -20px;
  height:auto;
}